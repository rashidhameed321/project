<VBox spacing="20.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo1.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <AmbientLight color="CHARTREUSE" lightOn="true" />
   <Label text="Upgenics International" />
   <CheckBox alignment="TOP_LEFT" mnemonicParsing="false" prefHeight="0.0" prefWidth="212.0" text="CheckBox" />

    <Label fx:id="welcome" />
    <Button onAction="#onHelloButtonClick" prefHeight="26.0" prefWidth="315.0" text="Hello!" />

</VBox>
